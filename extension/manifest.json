{
  "manifest_version": 2,
  "name": "CurrencyConverter",
  "version": "0.1",
  "permissions": ["storage"],
  "browser_action": {
	   "default_icon": "logo.png",
     "default_popup": "popup.html"
  },
  "content_scripts": [
     {
         "matches": ["<all_urls>"],
         "js": ["currencies.js","popup.js"]
     }
   ]
}
